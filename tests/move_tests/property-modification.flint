contract C {
  var v: V = V(2)
  var u: Int
}

C :: (any) {
  public init() {
    u = 1
    self.v = V(3) // Last thing to do
    v.x = 4
    v()
    u = 2
    let x: Int = u
    assert(u == x)
  }

  public func v() mutates(v.x) {
    var v: V = V(1)
    assert(v.x == 1)
    v.x = 1
  }
}

struct V {
  public var x: Int = 0

  init(x: Int) {
    self.x = x
  }

  func increment()
  mutates (x) {
    self.x += 1
  }
}