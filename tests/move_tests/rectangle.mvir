import {{default}}.Rectangle;

main(account: &signer) {
    Rectangle.publish(10, 20, copy(account));
    assert(Rectangle.area(copy(account)) == 200, 1);
    Rectangle.changeWidth(copy(account), 5);
    assert(Rectangle.area(copy(account)) == 100, 2);
    assert(Rectangle.changeArea(copy(account), 5, 10) == 50, 3);
    assert(Rectangle.area(copy(account)) == 50, 4);
    return;
}