contract Lottery {
    var winnings: Int = 1000
    var manager: Address = 0x0
}

Lottery :: (any) {
    public init() {}
}

Lottery :: (winner) {
    func winner() -> Address {
        return manager
    }

    func getWinnings() -> Int {
        return winnings
    }
}

Lottery :: (lucky) {
  func lucky(caller_address: Address) -> Bool {
    return true
  }

  func isWinner() -> Bool {
      return true
  }
}
