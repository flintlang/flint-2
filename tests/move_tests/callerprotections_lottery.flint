contract Lottery {
    var winnings: Int = 1000
    var players: [Address] = [0x0, 0x1, 0x2]
    var people: [Address: Address] = [0x0: 0x0, 0x1: 0x1, 0x2: 0x2]
    var manager: Address = 0x3
}

Lottery :: (any) {
    public init() {}
}

Lottery :: (winner, manager) {
    func winner() -> Address {
        return 0x2
    }

    func getWinnings() -> Int {
        return winnings
    }
}

Lottery :: (players, people) {
    func firstAddressIsWinner() -> Bool{
        return people[0x0] == 0x2
    }

    func addPerson() mutates (people) {
        people[0x3] = 0x3
    }
}

Lottery :: (lucky) {
  func lucky(caller_address: Address) -> Bool {
    return true
  }

  func isWinner() -> Bool {
      return true
  }
}
