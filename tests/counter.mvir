import {{default}}.Counter;

main(account: &signer) {
    Counter.publish(copy(account));
    assert(Counter.getValue(copy(account)) == 0, 1);
    Counter.increment(copy(account));
    Counter.increment(copy(account));
    assert(Counter.getValue(copy(account)) == 2, 1);
    Counter.decrement(copy(account));
    assert(Counter.getValue(copy(account)) == 1, 1);
    return;
}