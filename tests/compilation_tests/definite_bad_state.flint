contract MyContract (One, Two, Three) {
    var dummy: Int = 0
}

MyContract :: (any) {
    public init() {
      become One
    }
}

MyContract @(One, Three) :: (any) {
    public func foo() {
        //! compile fail line 14
        bar() // Should fail since we are not in state Two
    }
}

MyContract @(Two) :: (any) {
    public func bar() -> Int {
        return 10
    }
}
